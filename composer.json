{
  "name": "grokhotov/atol",
  "description": "ATOL PHP API Client. Данная библиотека предназначена для работы с сервисом АТОЛ Онлайн (v5)",
  "keywords": ["atol", "api", "online", "php"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Amir Dzhalilov",
      "email": "amir@positron-it.ru"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Grokhotov\\ATOL\\": "src/",
      "Grokhotov\\ATOL\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpcov": "^8.2.0",
    "codacy/coverage": "^1.4",
    "phpmd/phpmd": "^2.6"
  }
}
